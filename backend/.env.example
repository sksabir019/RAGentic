# Environment
NODE_ENV=development

# Backend Server
BACKEND_PORT=3000
CORS_ORIGIN=http://localhost:5175

# Database Configuration
DATABASE_TYPE=postgres

# PostgreSQL Configuration (only used if DATABASE_TYPE=postgres)
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=ragentic_dev
DB_SSL=false

# MongoDB Configuration (only used if DATABASE_TYPE=mongodb)
MONGODB_HOST=localhost
MONGODB_PORT=27017
MONGODB_USER=root
MONGODB_PASSWORD=root123
MONGODB_DATABASE=ragentic

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=redis123
REDIS_DB=0
# REDIS_URL=redis://localhost:6379

# Vector Store Configuration
VECTOR_DIMENSION=1536

# Worker Configuration
DOCUMENT_INGESTION_CONCURRENCY=2
DOCUMENT_INGESTION_RETRY_ATTEMPTS=3
DOCUMENT_INGESTION_RETRY_DELAY_MS=5000
DOCUMENT_INGESTION_RETAIN_FAILED=10

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_change_in_production
JWT_EXPIRATION=7d

# File Upload Configuration
UPLOAD_FOLDER=uploads
MAX_FILE_SIZE=52428800

# ============================================
# AI Provider Configuration
# Set at least ONE of these API keys to enable AI features
# ============================================

# OpenAI Configuration
# Get your key at: https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-your-openai-api-key-here

# Groq Configuration (Free alternative with fast inference)
# Get your key at: https://console.groq.com/keys
GROQ_API_KEY=gsk_your-groq-api-key-here

# AI Provider Settings (optional)
# AI_PROVIDER=openai  # or 'groq' - defaults to first available
# OPENAI_MODEL=gpt-4o-mini
# GROQ_MODEL=llama-3.1-70b-versatile

# Embedding Model (for document search)
# EMBEDDING_MODEL=text-embedding-3-small

# ============================================
# RAG Agent Microservices (optional)
# Uses local RAG if not set
# ============================================
# QUERY_PARSER_AGENT_URL=http://localhost:4001
# RETRIEVAL_AGENT_URL=http://localhost:4002
# RANKING_AGENT_URL=http://localhost:4003
# GENERATION_AGENT_URL=http://localhost:4004
# VALIDATION_AGENT_URL=http://localhost:4005
